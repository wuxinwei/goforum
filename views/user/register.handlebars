<!-- template: register -->
<head>
    <script src="https://ajax.aspnetcdn.com/ajax/jQuery/jquery-3.2.1.min.js"></script>
    <link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/bootstrap/4.0.0-beta/css/bootstrap.min.css" integrity="sha384-/Y6pD6FV/Vv2HJnA6t+vslU6fwYXjCFtcEpHbNJ0lyAFsXTsjBbfaDjzALeQsN6M" crossorigin="anonymous">
</head>
<div class="register" id="register">
    <div class="register-main" id="register-main">
        <div class="register-item" id="register-item">
            <div class="register-item-name">Username</div>
            <input class="register-item-username" type="text" value="username" name="username"/>
        </div>
        <div class="register-item" id="register-item">
            <div class="register-item-name">Password</div>
            <input class="register-item-password" type="password" value="password" name="password"/>
        </div>
        <div class="register-item" id="register-item">
            <div class="register-item-name">Name</div>
            <input type="text" value="name" name="name"/>
        </div>
        <div class="register-item" id="register-item">
            <div class="register-item-name">Gender</div>
            <input type="text" value="gender" name="gender"/>
        </div>
        <div class="register-item" id="register-item">
            <div class="register-item-name">Country</div>
            <input type="text" value="country" name="country"/>
        </div>
        <div class="register-item" id="register-item">
            <div class="register-item-name">Province</div>
            <input type="text" value="province" name="province"/>
        </div>
        <div class="register-item" id="register-item">
            <div class="register-item-name">City</div>
            <input type="text" value="city" name="city"/>
        </div>
        <div class="register-submit" id="register-submit">
            <input type="submit" value="Submit" name="submit" onsubmit="">
        </div>
    </div>
</div>

{{> footer }}
